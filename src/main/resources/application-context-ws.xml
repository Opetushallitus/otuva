<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:jaxws="http://cxf.apache.org/jaxws"
	xsi:schemaLocation="
	   http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
	   http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd
	">

    <jaxws:client id="authenticationService"
        serviceClass="fi.vm.sade.authentication.service.AuthenticationService" 
        address="${auth.webservice.url}" />

    <jaxws:client id="userManagementService"
        serviceClass="fi.vm.sade.authentication.service.UserManagementService"
        address="${usermanagement.webservice.url}" 
    />


	<bean id="organisaatioService" class="fi.vm.sade.organisaatio.api.OrganisaatioServiceMock"/>

</beans>
